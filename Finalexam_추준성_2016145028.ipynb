        "id": "FLk_TnmqMr1k"
      },
      "source": [
        "#alphable label\n",
        "from string import ascii_lowercase\n",
        "\n",
        "alphabet_list = list(ascii_lowercase)\n",
        "alphabel_label =[]\n",
        "for alphabet in alphabet_list:\n",
